{

	"manifest_version": 2,
	"name": "leet code page opener",
	"description": "leet code page opener",
	"version": "1",
	
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"browser_action": {
		"default_title": "Open"
	},

	"permissions": [
		"https://leetcode.com/problemset/*",
		"http://leetcode.com/problemset/*",
		"tabs",
		"activeTab"
	]

}